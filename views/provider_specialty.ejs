<table>
    <thead>
        <tr>
            <th>Provider Specialty</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        <% data.forEach(row => { %>
            <tr>
                <td><%= row.provider_specialty %></td>
                <td><%= row.first_name %></td>
                <td><%= row.last_name %></td>

            </tr>
        <% }); %>
    </tbody>
  </table>